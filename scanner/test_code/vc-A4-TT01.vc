main()
{
  // ----------------------------------------------
  //   SYMBOLIC CONSTANTS AND VARIABLES
  // ----------------------------------------------
    const  a1 = "GCD", c1 = '=';
    int	A, B, K;

  // ----------------------------------------------
  //   PROCESSING STATEMENTS
  // ----------------------------------------------

    print( "Please enter A: "); read(A);
    print( "Please enter B: " ); read(B);
    if (A < B) { K = A; A = B; B = K; }
    K = A % B;
    while(K != 0) { A = B; B = K; K = A % B; }
    print( a1 );
    print( c1 );
    println(B);

  // --- END OF PROGRAM ---------------------------  
}